{
  "name": "@chekushka/focus",
  "version": "2.0.0",
  "description": "A video codec, designed to be used in:\n1. Video conferencing systems (like Skype, Google Meet, Zoom, etc.)\n2. Streaming services (Twitch, YouTube)",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly -p ./tsconfig.build.json",
    "build:js": "webpack --mode=production",
    "build": "rimraf dist/ && npm run build:types && npm run build:js",
    "lint": "eslint '*/**/*.{js, ts, tsx, jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "author": {
    "name": "Chizha D.",
    "email": "awesome.chekushka@gmail.com"
  },
  "main": "dist/focus.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/awesome-ddp/focus.git"
  },
  "keywords": [
    "video",
    "codec",
    "video codec",
    "video compression"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/awesome-ddp/focus/-/issues"
  },
  "homepage": "https://gitlab.com/awesome-ddp/focus",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/gitlab": "^6.0.9",
    "@tsconfig/recommended": "^1.0.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "fork-ts-checker-webpack-plugin": "^6.2.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "source-map-loader": "^2.0.1",
    "typescript": "^4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
